---
import CartBase from './BaseCatrocarts.astro'
import CartsContent from '../../config/DataTecnologis.json'
---
<ul class="container pl-5 mb-10 xl:pl-10 xl:grid grid-cols-3 gap-20">
    {
        CartsContent.map((CartsContent)=>{
            return(
                <CartBase
                Title ={CartsContent.Tecnologia}
                Image ={CartsContent.Image}
                Content = {CartsContent.Content}
                />
            )
        })
    }
</ul>